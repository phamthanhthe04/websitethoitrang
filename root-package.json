{
  "name": "webquanao",
  "version": "1.0.0",
  "description": "Web bán quần áo với frontend React và backend Node.js/Express",
  "main": "index.js",
  "scripts": {
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "install:all": "npm i && cd frontend && npm i && cd ../backend && npm i",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["react", "nodejs", "ecommerce", "express"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
